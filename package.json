{
  "name": "seven-hand-poker",
  "version": "1.0.0",
  "main": "src/index.js",
  "author": "alexfaunt <alexfaunt@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node dist",
    "start:dev": "babel-node -r ./node_modules/dotenv/config ./src/",
    "build": "yarn clean && yarn build:api && yarn build:app && yarn build:client",
    "build:clean": "rm -rf ./dist",
    "build:api": "babel src/server -d dist/server --copy-files --ignore *.test.js,__mocks__/*.js",
    "build:app": "babel src/app -d dist/app --copy-files --ignore *.test.js,__mocks__/*.js",
    "build:client": "webpack-cli --config ./webpack-config/prod.js",
    "migrate": "babel-node -r ./node_modules/dotenv/config ./src/db/migrate",
    "rollback": "babel-node -r ./node_modules/dotenv/config ./src/db/rollback",
    "seed": "knex seed:run",
    "lint": "eslint --ignore-path .gitignore --cache .",
    "webpack-stats": "webpack-cli --config ./webpack-config/stats.js",
    "heroku-postbuild": "yarn build && yarn migrate",
    "prepush": "yarn lint"
  },
  "engines": {
    "node": "8.11.2",
    "yarn": "0.27.5"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/node": "^7.0.0-beta.49",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.49",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "apollo-cache-inmemory": "^1.2.1",
    "apollo-client": "^2.3.1",
    "apollo-link": "^1.2.2",
    "apollo-link-error": "^1.0.9",
    "apollo-link-http": "^1.5.4",
    "apollo-link-schema": "^1.1.0",
    "apollo-server-koa": "^1.3.6",
    "babel-plugin-module-resolver": "^3.1.1",
    "convict": "^4.2.0",
    "dotenv": "^5.0.1",
    "graphql": "^0.13.2",
    "knex": "^0.14.6",
    "koa": "^2.5.1",
    "koa-bodyparser": "next",
    "koa-router": "next",
    "objection": "^1.1.8",
    "objection-graphql": "^0.4.0",
    "pg": "^7.4.3",
    "react": "^16.4.0",
    "react-apollo": "^2.1.4",
    "react-dom": "^16.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-eslint": "8",
    "babel-loader": "^8.0.0-beta",
    "eslint": "4.x",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-babel-module": "5.0.0-beta.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.8.2",
    "husky": "^0.14.3",
    "koa-webpack": "^4.0.0",
    "webpack": "^4.10.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.1.4",
    "webpack-merge": "^4.1.2"
  }
}
