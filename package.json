{
  "name": "seven-hand-poker",
  "version": "1.0.0",
  "main": "src/index.js",
  "author": "alexfaunt <alexfaunt@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production node dist",
    "start:dev": "NODE_ENV=development nodemon -r ./node_modules/dotenv/config ./src --inspect=0.0.0.0:9229 --exec babel-node",
    "build": "yarn build:api",
    "build:api": "rm -rf ./dist && babel src -d dist --copy-files --ignore *.test.js,__mocks__/*.js",
    "migrate": "babel-node -r ./node_modules/dotenv/config ./src/db/migrate",
    "rollback": "babel-node -r ./node_modules/dotenv/config ./src/db/rollback",
    "seed": "knex seed:run",
    "lint": "eslint --ignore-path .gitignore --cache .",
    "postinstall": "yarn build && yarn migrate",
    "prepush": "yarn lint"
  },
  "engines": {
    "node": "8.11.2",
    "yarn": "0.27.5"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/node": "^7.0.0-beta.49",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.49",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "apollo-server-koa": "^1.3.6",
    "babel-plugin-module-resolver": "^3.1.1",
    "convict": "^4.2.0",
    "dotenv": "^5.0.1",
    "graphql": "^0.13.2",
    "knex": "^0.14.6",
    "koa": "^2.5.1",
    "koa-bodyparser": "next",
    "koa-router": "next",
    "nodemon": "^1.17.5",
    "objection": "^1.1.8",
    "objection-graphql": "^0.4.0",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "babel-eslint": "8",
    "eslint": "4.x",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-babel-module": "5.0.0-beta.0",
    "eslint-plugin-filenames": "^1.2.0",
    "eslint-plugin-import": "^2.12.0",
    "husky": "^0.14.3"
  }
}
